{
  "name": "loja-integrada-carrinho",
  "version": "1.0.0",
  "author": "Vinicius Meneses <vinicius.meneses04@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect --respawn src/main/server.ts",
    "test": "jest --passWithNoTests",
    "test:unit": "yarn test --watch",
    "test:coverage": "yarn test --coverage && jest-coverage-badges output './badges'",
    "test:e2e": "yarn test --watch --silent --runInBand -c jest-e2e.config.js",
    "migration:create": "typeorm migration:create",
    "migration:run": "yarn typeorm:data-source migration:run",
    "migration:revert": "yarn typeorm:data-source migration:revert",
    "typeorm:data-source": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/infra/database/postgres/dataSource.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.0.2",
    "jest-coverage-badges": "^1.1.2",
    "pg": "^8.7.3",
    "rambda": "^7.1.4",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.4.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.3.6"
  }
}
